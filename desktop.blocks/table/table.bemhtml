block('table')(
    js()(true),
    tag()('table'),
    elem('head')(
    	tag()('thead'),
    	mix()({ block : 'table-head' })
    ),
    elem('row')(
    	tag()('tr')
    ),
    elem('body')(
        tag()('tbody')
    ),
    elem('cell')(
    	tag()('td'),
    	elemMod('type', 'heading')(
    		tag()('th')
    	)
    ),
    elem('image')(
        tag()('img'),
        attrs()(function(){
            return { src : this.ctx.url, title : this.ctx.title }
        })
    )
)
